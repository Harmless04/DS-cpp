class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        unordered_map<int , int>freq;
        for(int num : arr){
freq[num]++;
        }
        unordered_set<int>counts;
        for(auto it : freq){
            if(counts.find(it.second) != counts.end())
            return false;
            counts.insert(it.second);
}
return true;
        

    }};